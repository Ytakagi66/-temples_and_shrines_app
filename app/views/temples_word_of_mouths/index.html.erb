<p id="notice"><%= notice %></p>

<h1>Temples Word Of Mouths</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Article</th>
      <th>Photo</th>
      <th>User</th>
      <th>Temple</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temples_word_of_mouths.each do |temples_word_of_mouth| %>
      <tr>
        <td><%= temples_word_of_mouth.name %></td>
        <td><%= temples_word_of_mouth.article %></td>
        <td><%= temples_word_of_mouth.photo %></td>
        <td><%= temples_word_of_mouth.user_id %></td>
        <td><%= temples_word_of_mouth.temple_id %></td>
        <td><%= link_to 'Show', temples_word_of_mouth %></td>
        <td><%= link_to 'Edit', edit_temples_word_of_mouth_path(temples_word_of_mouth) %></td>
        <td><%= link_to 'Destroy', temples_word_of_mouth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

