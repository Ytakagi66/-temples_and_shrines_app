<p id="notice"><%= notice %></p>
<h1>User Ids</h1>
  <% @user_ids.each do |user_id| %>
    <tr>
      <td><%= user_id.name %></td>
      <td><%= user_id.old %></td>
      <td><%= user_id.email %></td>
      <td><%= user_id.password %></td>
      <td><%= link_to 'Show', user_id %></td>
      <td><%= link_to 'Edit', edit_user_id_path(user_id) %></td>
      <td><%= link_to 'Destroy', user_id, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
<br>
<%= render @user_ids %>
<%= link_to 'New User', new_user_id_path %>
